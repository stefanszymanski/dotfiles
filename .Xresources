!! urxvt settings

URxvt*termName:		    xterm-256color
URxvt*geometry:			273x78
URxvt*loginShell:		true
! basic behaviour
URxvt*scrollTtyKeyPress:	true
URxvt*scrollWithBuffer:		false
URxvt*skipScroll:		true
URxvt*visualBell:		true
URxvt*saveLines:		0
! appearance
URxvt*scrollBar:		false
URxvt*internalBorder:		6
URxvt*fading:			30
URxvt*intensityStyles: false
! fonts
URxvt*letterSpace:		0
URxvt*lineSpace:		0
! Inconsolata works with letterSpace=0 relativly nice, the other fonts always have some bug when
! rendering powerline symbols
! The only really working font is SCP Medium
!URxvt*font: 			xft:Inconsolata for Powerline:size=10,xft:DejaVu Sans Mono for Powerline:size=10
!URxvt*font: 			xft:SauceCodePro Nerd Font:pixelsize=13:antialias=true:hinting=true
!URxvt*font: 			xft:monospace:pixelsize=13:antialias=true:hinting=true
URxvt*font:             xft:Hack:size=10,xft:Hack Nerd Font:size=10
! misc
URxvt*print-pipe:		cat > $HOME/$(echo urxvt.dump.$(date + '%Y%M%d%H%m%S'))


