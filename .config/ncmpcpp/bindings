def_key "j"
    scroll_down
def_key "k"
    scroll_up
def_key "h"
    previous_column
def_key "l"
    next_column

def_key "J"
    select_item
    scroll_down
def_key "K"
    select_item
    scroll_up

def_key "ctrl-u"
    page_up
def_key "ctrl-b"
    page_up
def_key "ctrl-d"
    page_down
def_key "ctrl-f"
    page_down

def_key "n"
    next_found_item
def_key "N"
    previous_found_item

def_key "g"
    move_home
def_key "G"
    move_end

def_key "ctrl-n"
    next
def_key "ctrl-p"
    previous

def_key "+"
    volume_up
def_key "-"
    volume_down

def_key "d"
    delete_playlist_items

def_key "P"
    jump_to_position_in_song

def_key "L"
    show_lyrics
