Include config.d/*

Host *
    ExitOnForwardFailure yes
    ControlPath ~/.ssh/control/%h-%p-%r
    ControlMaster auto
    ControlPersist 10m
    ServerAliveInterval 60
    ServerAliveCountMax 5
    TCPKeepAlive no
    IPQos throughput
    IdentitiesOnly no
    PubkeyAuthentication yes
    PasswordAuthentication yes
    IdentityFile ~/.ssh/personal.pub


#######
# VCS #
#######

Host bitbucket
    Hostname bitbucket.org
    User git

Host github
    Hostname github.com
    User git
